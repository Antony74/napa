{
  "name": "napa",
  "description": "A helper for installing repos without a package.json with npm.",
  "version": "2.0.2",
  "homepage": "https://github.com/shama/napa",
  "contributors": [
    "Kyle Robinson Young <kyle@dontkry.com> (http://dontkry.com)",
    "Casey Webb <notcaseywebb@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/shama/napa.git"
  },
  "bugs": {
    "url": "https://github.com/shama/napa/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 4.0.0"
  },
  "bin": {
    "napa": "bin/napa"
  },
  "main": "lib",
  "scripts": {
    "build": "babel src --out-dir lib",
    "format": "standard",
    "test": "istanbul cover --report lcov --print detail test.js",
    "coverage": "istanbul report html && open ./coverage/index.html"
  },
  "dependencies": {
    "bluebird": "^3.0.2",
    "cli-spinner": "^0.2.1",
    "download": "^4.4.1",
    "mkdirp": ">=0.5.0 <0.6.0-0",
    "npm-cache-filename": ">=1.0.1 <1.1.0-0",
    "npmlog": ">=1.2.0 <1.3.0-0",
    "rimraf": ">=2.4.0 <2.5.0-0",
    "tar-pack": ">=2.0.0 <2.1.0-0"
  },
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "codeclimate-test-reporter": "^0.1.1",
    "istanbul": "^0.3.22",
    "pre-commit": "^1.1.1",
    "standard": "^5.3.1",
    "tape": ">=4.2.0 <4.3.0-0"
  },
  "keywords": [
    "npm",
    "install",
    "assets"
  ],
  "napa": {
    "foo": "foo/repo",
    "ember": "https://github.com/emberjs/ember.js/archive/v1.7.0.tar.gz",
    "handlebars": "components/handlebars.js",
    "flippant": "mintchaos/flippant.js#gh-pages",
    "jquery.inview": "zuk/jquery.inview#nonexistent-branch"
  },
  "napa-config": {
    "cache": false
  },
  "pre-commit": [
    "format",
    "build"
  ],
  "standard": {
    "ignore": [
      "lib"
    ]
  },
  "optionalDependencies": {
    "ember": "file:napa_modules/ember",
    "flippant": "file:napa_modules/flippant",
    "handlebars": "file:napa_modules/handlebars",
    "jquery.inview": "file:napa_modules/jquery.inview"
  }
}
